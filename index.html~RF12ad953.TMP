		<script type="text/javascript"> 
			
		function init_map()
			{				
				var myOptions = {zoom:14,center:new google.maps.LatLng(12.966684701995554,77.71181278295899),mapTypeId: google.maps.MapTypeId.ROADMAP};
				map = new google.maps.Map(document.getElementById("gmap_canvas"), myOptions);
				marker = new google.maps.Marker({map: map,position: new google.maps.LatLng(12.966684701995554, 77.71181278295899)});
				infowindow = new google.maps.InfoWindow({content:"<b>CMRIT</b><br/>AECS Layout , ITPL Main Road <br/>560037 Bangalore" });
				google.maps.event.addListener(marker, "click", function(){infowindow.open(map,marker);});infowindow.open(map,marker);
				
				
			}
			
			
				google.maps.event.addDomListener(window, 'load', init_map);
		</script>
